{
    "name": "chippyash/slim-dic",
    "description": "Symfony DIC integration for the Slim Framework",
    "homepage" : "https://github.com/chippyash/Slim-Dic",
    "license": "GPL 3.0",
    "keywords": ["symfony","dependency","injection","slim","web"],
    "authors": [
        {
            "name": "Ashley Kitson",
            "email": "ashley@zf4.biz"
        }
    ],
    "require": {
        "php": ">=5.5",
        "slim/slim": "~2.0",
        "symfony/filesystem": "~2.6",
        "symfony/dependency-injection": "~2.6",
        "symfony/config": "~2.6",
        "chippyash/strong-type": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.3",
        "satooshi/php-coveralls" : "dev-master",
        "phpunit/phpunit-skeleton-generator": "~2.0",
        "apigen/apigen": "2.8.1"
    },
    "autoload": {
        "psr-0": {
            "Slimdic\\": "src"
        }
    }
}
